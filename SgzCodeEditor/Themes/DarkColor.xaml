<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background-->
    <SolidColorBrush x:Key="MainBackground" Color="#FF1E1E1E" /> <!--255 30 30 30-->
    <SolidColorBrush x:Key="MainBorderBrush" Color="Gray" /> <!--255 128 128 128-->
    <SolidColorBrush x:Key="MainForeground" Color="#FFC8C8C8" /> <!--255 200 200 200-->
    <SolidColorBrush x:Key="MainMouseHover" Color="#FF282828" /> <!--255 255 255 255-->
    <SolidColorBrush x:Key="MainMax" Color="White" /> <!--255 255 255 255-->
    <SolidColorBrush x:Key="MainMin" Color="Black" /> <!--255 255 255 255-->
    
    
    <!--TreeView-->
    <SolidColorBrush x:Key="TreeViewBackground" Color="#FF252526" /> <!--255 37 37 38-->
    
    
    <!--Toolbar-->
    <SolidColorBrush x:Key="ToolbarBackground" Color="#FF2D2D30" /> <!--255 51 51 51-->
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#FF3C3C3C" />
    <SolidColorBrush x:Key="ToolbarMouseHover" Color="#FF3C3C3F" />

    
    <!--Disable-->
    <SolidColorBrush x:Key="DisabledForeground" Color="#656565" />


</ResourceDictionary>