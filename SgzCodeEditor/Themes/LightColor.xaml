<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background-->
    <SolidColorBrush x:Key="MainBackground" Color="#FFE1E1E1" /> <!--255 30 30 30-->
    <SolidColorBrush x:Key="MainBorderBrush" Color="Gray" /> <!--255 128 128 128-->
    <SolidColorBrush x:Key="MainForeground" Color="#FF191919" /> <!--255 200 200 200-->
    <SolidColorBrush x:Key="MainMouseHover" Color="#FFC8C8C8" /> <!--255 255 255 255-->
    <SolidColorBrush x:Key="MainMax" Color="Black" /> <!--255 255 255 255-->
    <SolidColorBrush x:Key="MainMin" Color="White" /> <!--255 255 255 255-->
    
    <!--TreeView-->
    <SolidColorBrush x:Key="TreeViewBackground" Color="#FFDADAD9" /> <!--255 37 37 38-->
    
    
    <!--Toolbar-->
    <SolidColorBrush x:Key="ToolbarBackground" Color="#FFCCCCCC" /> <!--255 51 51 51-->
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#FFAFAFAF" />
    <SolidColorBrush x:Key="ToolbarMouseHover" Color="#FFA5A5A5" />

    
    <!--Disable-->
    <SolidColorBrush x:Key="DisabledForeground" Color="#656565" />

</ResourceDictionary>